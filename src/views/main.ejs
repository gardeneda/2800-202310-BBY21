                  </svg>
                  Time</h5>
                <p id="time"></p>
                <br>
              </div>
            </div>
          </div>
          <br>
          <!--Suggestions Card-->
          <div class="card full-card" id="suggestionsCard">
            <div class="card-header">
              Suggestions
            </div>
            <div class="card-body">
              <%- include("templates/todoTemplate") %>
              <div class="empty-div">&nbsp;</div>
              <a id="todo-btn" href="/todo" class="btn btn-primary">View</a>
            </div>
          </div>
        </div>

