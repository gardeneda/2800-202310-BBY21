    <body>

      <!--Navbar-->
      <%- include("templates/header") %>
      <!--/.Navbar-->
      <br>
      <!--ChatBot Button-->
        <div class="parent">
          <audio id="audio" src="/img/sounds.wav" muted></audio>
          <button id="chatButton" class="round-6" 
          ontouchstart="handleInteraction(true)"
          ontouchend="stopSpin()"
          onmousedown="handleInteraction(false)"
          onmouseup="stopSpin()"
          onclick="location.href='chat'; return false;">
